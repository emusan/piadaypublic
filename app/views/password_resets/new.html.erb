<% provide :title,"Reset Password" %>
<h1>Reset Password</h1>

<%= form_tag password_resets_path,method: :post do %>
  <div class="form">
    <%= label_tag :email %>
    <%= text_field_tag :email,params[:email] %>
  </div>
  <div class="actions"><%= submit_tag "Reset Password",class: "button" %></div>
<% end %>
